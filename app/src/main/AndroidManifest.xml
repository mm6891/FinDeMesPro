<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="globalsolutions.findemes" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/titulo"
        android:theme="@style/AppTheme"
        android:versionCode="1"
        android:versionName="1.0.0">
        <activity
            android:name=".pantallas.activity.Password">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".pantallas.activity.MainActivity"
            android:label="@string/titulo" >
        </activity>
        <activity
            android:name=".pantallas.activity.GastoActivity"
            android:label="@string/MENU_GASTOS" >
        </activity>
        <activity
            android:name=".pantallas.activity.IngresoActivity"
            android:label="@string/MENU_INGRESOS" >
        </activity>
        <activity
            android:name=".pantallas.activity.MovimientosActivity"
            android:label="@string/MENU_MOVIMIENTOS" >
        </activity>
        <activity
            android:name=".pantallas.activity.InformesActivity"
            android:label="@string/MENU_INFORMES" >
        </activity>
        <activity
            android:name=".pantallas.activity.OpcionesActivity"
            android:label="@string/MENU_OPCIONES" >
        </activity>
        <activity
            android:name=".pantallas.activity.RegistrosActivity"
            android:label="@string/MENU_REGISTROS" >
        </activity>
        <activity
            android:name=".pantallas.activity.OptionActivityPassword"
            android:label="@string/OPCIONES_CONTRASENYA" >
        </activity>
        <activity
            android:name=".pantallas.activity.OptionActivityDatabase"
            android:label="@string/OPCIONES_BASEDATOS" >
        </activity>
        <activity
            android:name=".pantallas.activity.OptionActivityAyuda"
            android:label="@string/OPCIONES_AYUDA" >
        </activity>
        <activity
            android:name=".pantallas.activity.OptionActivityBarChart"
            android:label="@string/MENU_INFORMES"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name="com.dropbox.sync.android.DbxAuthActivity" />
        <activity
            android:name="com.dropbox.client2.android.AuthActivity"
            android:launchMode="singleTask" >
            <intent-filter>
                <data android:scheme="db-APP_KEY" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <service
            android:name="com.dropbox.sync.android.DbxSyncService"
            android:enabled="true"
            android:exported="false"
            android:label="Dropbox Sync" />
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
</manifest>
